{
  "name": "@pixelmakers/elements",
  "version": "0.1.11",
  "type": "module",
  "description": "A collection of custom web components by PixelMakers",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js"
    },
